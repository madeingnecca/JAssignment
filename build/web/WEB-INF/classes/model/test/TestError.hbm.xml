<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model.test">
  <class name="TestError" table="testerrors">
    <id column="id" name="id" type="java.lang.Integer">
      <generator class="sequence"/>
    </id>
    <property column="line" name="line" type="java.lang.Integer"/>
    <property column="error" name="error" type="java.lang.String"/>
    <property column="code" name="code" type="java.lang.String"/>
    <property column="file" name="file" type="java.lang.String"/>
    <property column="type" name="type" type="java.lang.Integer"/>
  </class>
</hibernate-mapping>
