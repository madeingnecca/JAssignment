package model;

import java.io.Serializable;

public class Submission implements Serializable {
    
    private Student         login;
    private String          exerciseText;
    private String          pseudoText;
    private Text            text;
    
    private Result result;

    
    public Submission() {
        
    }

    public Text getText() {
        return text;
    }
    
    public void setText( Text t ) {
        text = t;
    }
    
    public Student getLogin() {
        return login;
    }

    public void setLogin(Student login) {
        this.login = login;
    }


    public String getExerciseText() {
        return exerciseText;
    }

    public void setExerciseText(String exerciseText) {
        this.exerciseText = exerciseText;
    }

    public String getPseudoText() {
        return pseudoText;
    }

    public void setPseudoText(String pseudoText) {
        this.pseudoText = pseudoText;
    }

    public Result getResult() {
        return result;
    }

    public void setResult(Result result) {
        this.result = result;
    }
}
